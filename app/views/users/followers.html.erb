<% @users.each do |user|%>
    <div class="profiel_img">
        <%= link_to image_tag("/user_images/#{user.profiel_image}") , "/users/#{user.id}" %>
    </div>
    <div class="profiel_name">
        <%= user.name %>
    </div>
    <% if user != @current_user%>
        <% if user.is_followed_by?(@current_user)%>
            <%= link_to "フォロー解除",user_relationship_path(user), data: { turbo_method: :delete}%>
        <% else %>
            <%= link_to "フォロー",user_relationships_path(user), data: { turbo_method: :post}%>
        <% end %>
    <% end %>
<% end %>